{
  "name": "micro-list",
  "version": "0.5.0",
  "description": "Single-command HTTP directory listing and file serving",
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp build",
    "test": "eslint bin/**"
  },
  "files": [
    "dist",
    "views"
  ],
  "repository": "zeit/micro-list",
  "babel": {
    "presets": [
      "es2015-node"
    ],
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions"
    ]
  },
  "bin": {
    "list": "dist/bin/list"
  },
  "eslintConfig": {
    "extends": "default",
    "parser": "babel-eslint"
  },
  "keywords": [
    "now",
    "serve",
    "micro",
    "http-server"
  ],
  "author": "leo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeit/micro-list/issues"
  },
  "homepage": "https://github.com/zeit/micro-list#readme",
  "devDependencies": {
    "babel-eslint": "6.1.0",
    "babel-plugin-syntax-async-functions": "6.8.0",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-preset-es2015-node": "4.0.2",
    "eslint": "3.0.0",
    "eslint-config-default": "0.2.1",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-cached": "1.1.0",
    "gulp-chmod": "1.3.0",
    "gulp-sass": "2.3.2"
  },
  "dependencies": {
    "args": "1.3.0",
    "chalk": "1.1.3",
    "denodeify": "1.2.1",
    "filesize": "3.3.0",
    "handlebars": "4.0.5",
    "istextorbinary": "2.1.0",
    "micro-core": "0.4.0",
    "mime": "1.3.4"
  },
  "greenkeeper": {
    "emails": false,
    "ignore": [
      "babel-preset-es2015-node"
    ]
  }
}

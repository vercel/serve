{
  "name": "serve",
  "version": "3.2.2",
  "description": "Static file serving and directory listing",
  "scripts": {
    "precommit": "npm run test",
    "test": "xo"
  },
  "files": [
    "bin",
    "lib",
    "assets",
    "views"
  ],
  "repository": "zeit/serve",
  "bin": {
    "serve": "./bin/serve.js"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "rules": {
      "babel/no-await-in-loop": 0,
      "import/no-unresolved": 0,
      "complexity": 0,
      "import/no-unassigned-import": 0,
      "unicorn/no-process-exit": 0,
      "max-params": 0,
      "camelcase": 0
    }
  },
  "keywords": [
    "now",
    "serve",
    "micro",
    "http-server"
  ],
  "author": "leo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeit/serve/issues"
  },
  "homepage": "https://github.com/zeit/serve#readme",
  "devDependencies": {
    "husky": "^0.13.1",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "args": "^2.2.4",
    "async-to-gen": "^1.3.2",
    "basic-auth": "^1.1.0",
    "chalk": "^1.1.3",
    "copy-paste": "^1.3.0",
    "detect-port": "^1.1.0",
    "filesize": "^3.5.4",
    "fs-promise": "^1.0.0",
    "handlebars": "^4.0.6",
    "ip": "^1.1.4",
    "is-async-supported": "^1.2.0",
    "micro": "^7.0.0",
    "micro-compress": "^1.0.0",
    "mime-types": "^2.1.12",
    "node-version": "^1.0.0",
    "path-type": "^2.0.0",
    "send": "^0.14.2",
    "update-notifier": "^1.0.3"
  }
}
